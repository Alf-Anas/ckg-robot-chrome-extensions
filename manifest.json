{
    "manifest_version": 3,
    "name": "CKG Robot",
    "version": "1.0",
    "description": "Robot automasi Cek Kesehatan Gizi Gratis",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "permissions": ["storage", "scripting", "activeTab", "tabs"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["helper.js"]
        }
    ]
}
